{
  "name": "symbiota2",
  "version": "0.0.0",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "build": "npm run build:all",
    "build:api": "nx build api",
    "build:ui": "nx build ui",
    "build:all": "nx run-many --target=build --projects=api,ui --parallel",
    "dep-graph": "nx dep-graph",
    "e2e": "nx e2e",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "help": "nx help",
    "i18n": "node ./bin/merge-translations.js",
    "i18n:clean": "node ./bin/clean-translations.js",
    "i18n:init": "node ./bin/init-translations.js",
    "lint": "nx workspace-lint && nx lint",
    "nx": "nx",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "sql": "cross-env TS_NODE_PROJECT=tsconfig.cli.json node -r ts-node/register -r tsconfig-paths/register -r typeorm --require glob bin/run-sql.ts",
    "start": "concurrently -r npm:start:prod:*",
    "start:dev": "concurrently -r npm:start:dev:*",
    "start:dev:ui": "nx serve ui",
    "start:dev:api": "nx serve api",
    "start:prod:ui": "nx serve ui --prod --watch=false --liveReload=false",
    "start:prod:api": "nx serve api --watch=false",
    "test": "nx run-many --target=test --all",
    "test:api": "nx run-many --target=test --projects=api --with-deps",
    "test:ui": "nx run-many --target=test --projects=ui --with-deps",
    "test:cov": "nx run-many --target=test --all",
    "typeorm": "cross-env TS_NODE_PROJECT=tsconfig.cli.json node -r ts-node/register -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f ormconfig.ts",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.4",
    "@angular/cdk": "^11.2.3",
    "@angular/common": "^11.2.0",
    "@angular/compiler": "^11.2.0",
    "@angular/core": "^11.2.0",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^11.2.0",
    "@angular/localize": "^11.2.4",
    "@angular/material": "^11.2.3",
    "@angular/platform-browser": "^11.2.0",
    "@angular/platform-browser-dynamic": "^11.2.0",
    "@angular/router": "^11.2.0",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/swagger": "^4.7.15",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "JSONStream": "^1.3.5",
    "class-transformer": "~0.3.1",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "csv-parser": "^3.0.0",
    "helmet": "^4.4.1",
    "jwt-decode": "^3.1.2",
    "mysql": "^2.18.1",
    "passport": "^0.4.1",
    "passport-cookie": "^1.0.9",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.6.3",
    "swagger-ui-express": "^4.1.6",
    "tslib": "^2.0.0",
    "typeorm": "~0.2.31",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.0",
    "@angular-eslint/eslint-plugin": "^1.2.0",
    "@angular-eslint/eslint-plugin-template": "^1.2.0",
    "@angular-eslint/template-parser": "^2.0.2",
    "@angular/compiler-cli": "^11.2.0",
    "@angular/language-service": "^11.2.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@nrwl/angular": "^11.4.0",
    "@nrwl/cli": "11.4.0",
    "@nrwl/cypress": "11.4.0",
    "@nrwl/devkit": "^11.4.0",
    "@nrwl/eslint-plugin-nx": "11.4.0",
    "@nrwl/jest": "11.4.0",
    "@nrwl/linter": "11.4.0",
    "@nrwl/nest": "^11.4.0",
    "@nrwl/node": "11.4.0",
    "@nrwl/tao": "11.4.0",
    "@nrwl/workspace": "11.4.0",
    "@types/express": "~4.17.3",
    "@types/jest": "26.0.8",
    "@types/multer": "~1.4.5",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "cypress": "^6.0.1",
    "dotenv": "6.2.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.2.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "glob": "^7.1.6",
    "jest": "26.2.2",
    "jest-preset-angular": "8.3.2",
    "ng-packagr": "^11.0.0",
    "prettier": "2.2.1",
    "ts-jest": "26.4.0",
    "ts-node": "~9.1.1",
    "typescript": "~4.1.2"
  }
}
