  "scripts": {
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "build": "npm run build:all",
    "build:api": "nx build api",
    "build:ui": "nx build ui",
    "build:all": "nx run-many --target=build --projects=api,ui --parallel",
    "dep-graph": "nx dep-graph",
    "e2e": "nx e2e",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "help": "nx help",
    "i18n": "node ./bin/merge-translations.js",
    "i18n:clean": "node ./bin/clean-translations.js",
    "i18n:init": "node ./bin/init-translations.js",
    "lint": "nx workspace-lint && nx lint",
    "nx": "nx",
    "postinstall": "ngcc --properties es2015 browser module main",
    "sql": "cross-env TS_NODE_PROJECT=apps/api/tsconfig.app.json node -r ts-node/register -r tsconfig-paths/register -r typeorm --require glob bin/run-sql.ts",
    "start": "cross-env NODE_ENV=production nx run-many --target=serve --projects=api,ui --parallel --watch=false",
    "start:dev": "cross-env NODE_ENV=development nx run-many --target=serve --projects=api,ui --parallel",
    "start:dev:ui": "nx serve ui",
    "start:dev:api": "nx serve api",
    "start:prod:ui": "nx serve ui --prod --watch=false --liveReload=false",
    "start:prod:api": "NODE_ENV=production nx serve api --watch=false",
    "test": "nx run-many --target=test --all",
    "test:api": "nx run-many --target=test --projects=api --with-deps",
    "test:ui": "nx run-many --target=test --projects=ui --with-deps",
    "test:cov": "nx run-many --target=test --all",
    "typeorm": "cross-env TS_NODE_PROJECT=./apps/api/tsconfig.app.json node -r ts-node/register -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f ormconfig.ts",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator"
  },
  "private": true,
